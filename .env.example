# Zilliz配置
# 请在 https://cloud.zilliz.com 注册账号并创建集群
ZILLIZ_ENDPOINT=https://your-cluster-endpoint.zillizcloud.com
ZILLIZ_TOKEN=your_zilliz_token_here

# Qwen/通义千问配置
# 请在 https://dashscope.console.aliyun.com 获取API密钥
QWEN_API_KEY=your_qwen_api_key_here
DASHSCOPE_API_KEY=your_dashscope_api_key_here
QWEN_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1

# OpenAI配置（备选）
# 如果不使用Qwen，可以配置OpenAI
OPENAI_API_KEY=your_openai_api_key_here

# Embedding模型配置
# 可选值: qwen_v3, openai_small
EMBEDDING_MODEL_TYPE=qwen_v3

# 数据存储路径
DATA_DIR=./data